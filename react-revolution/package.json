{
  "name": "react-revolution",
  "version": "1.1.0",
  "description": "React Revolution - build interactive modules for react projects",
  "main": "public/react-revolution.js",
  "scripts": {
    "build": "node ./node_modules/.bin/webpack --config webpack.config.production.js --module-bind js=babel-loader --progress --profile --colors",
    "build:dev": "node node_modules/webpack/bin/webpack.js --config webpack.config.production.js --module-bind js=babel-loader --watch-poll --watch --progress --profile --colors"
  },
  "keywords": [
    "React",
    "Accordion",
    "Breadcrumbs",
    "Cards ",
    "Clipboard",
    "GlobalMessages",
    "Icons ",
    "InputAnimation",
    "File",
    "DragDrop",
    "InputSuggestion",
    "Loading",
    "Pagination",
    "Menu",
    "PopupBox",
    "SideBar",
    "Table",
    "TextWriter"
  ],
  "author": "David Janitzek",
  "email": "david.janitzek@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-polyfill": "^6.26.0",
    "terser-webpack-plugin": "^1.4.1",
    "webpack-cli": "^3.3.10"
  }
}
