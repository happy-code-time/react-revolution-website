{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "lib": [
            "dom", 
            "es2015"
        ],
        "experimentalDecorators": true,
        "types": [
            "node"
        ],
    },
    "include": [
        "_Modules"
    ],
    "exclude": [
        "Distribution",
        "Images"
    ]
}