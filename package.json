{
  "name": "davids-react-revolution",
  "version": "5.4.5",
  "description": "Create fast, modern and responsive website components",
  "main": "website.jsx",
  "scripts": {
    "build:module:dev": "npm run module && npm run build:website:sass:prod && node node_modules/webpack/bin/webpack.js --config webpack.module.js --module-bind js=babel-loader --watch-poll --watch --progress --profile --colors --mode development",
    "build:website": "npm run build:website:sass:prod && node node_modules/webpack/bin/webpack.js --config webpack.website.js --module-bind js=babel-loader --progress --profile --colors --mode production --env min",
    "build:website:dev": "npm run build:website:sass:prod && npm run build:website:sass:dev & node node_modules/webpack/bin/webpack.js --config webpack.website.js --module-bind js=babel-loader --watch-poll --watch --progress --profile --colors --mode development",
    "server": "lite-server -c bs-config.json",
    "compile": "./node_modules/.bin/gulp compile",
    "module": "./node_modules/.bin/gulp module",
    "chmod:2775": "sudo chmod 2775 -R * && sudo chmod 775 -R *",
    "build:website:sass:dev": "node ./node_modules/.bin/node-sass --watch Website/Scss -o public/css",
    "build:website:sass:prod": "node ./node_modules/.bin/node-sass Website/Scss -o public/css"
  },
