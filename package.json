{
  "name": "davids-react-revolution",
  "version": "1.1.6",
  "description": "Create fast modern responisive websites elements",
  "main": "index.jsx",
  "scripts": {
    "build:module": "node ./node_modules/.bin/webpack --config webpack.module.production.js --mode production",
    "build:module:dev": "node node_modules/webpack/bin/webpack.js --config webpack.module.development.js --module-bind js=babel-loader --watch-poll --watch --progress --profile --colors",
    "build:website": "node node_modules/webpack/bin/webpack.js --config webpack.website.development.js --module-bind js=babel-loader --progress --profile --colors",
    "build:website:dev": "node node_modules/webpack/bin/webpack.js --config webpack.website.development.js --module-bind js=babel-loader --watch-poll --watch --progress --profile --colors",
    "server": "lite-server -c bs-config.json",
    "compile": "./node_modules/.bin/gulp compile",
    "watch:sass": "./node_modules/.bin/gulp watch:sass",
    "chmod:2775": "sudo chmod 2775 -R public/*",
    "watch": "npm run server & npm run build:website:dev & npm run watch:sass"
  },
  "keywords": [],
  "author": "David Janitzek",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "gulp-run": "^1.7.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.6",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-polyfill": "^6.26.0",
    "del": "^5.1.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-sass": "^4.1.0",
    "gulp4-run-sequence": "^1.0.1",
    "imagemin-jpeg-recompress": "^7.0.0",
    "lite-server": "^2.5.4",
    "react-revolution": "^1.1.6",
    "terser-webpack-plugin": "^1.4.4",
    "webpack-cli": "^3.3.10",
    "webpack-obfuscator": "^1.4.0"
  }
}
